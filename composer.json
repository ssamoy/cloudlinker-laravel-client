{
    "name": "stesa/cloudlinker-laravel-client",
    "description": "Laravel package for integrating with the Cloudlinker.eu API - connect printers, scanners, scales and IoT devices to your Laravel application",
    "keywords": ["laravel", "cloudlinker", "api", "printer", "iot", "scanner", "scale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stesa",
            "email": "info@stesa.nl"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Stesa\\CloudlinkerClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stesa\\CloudlinkerClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stesa\\CloudlinkerClient\\CloudlinkerServiceProvider"
            ],
            "aliases": {
                "Cloudlinker": "Stesa\\CloudlinkerClient\\Facades\\Cloudlinker"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
